# UniFi Network API Configuration
# Required settings for API access
UNIFI_API_KEY=your_api_key_here
UNIFI_HOST=unifi.local

# Optional API settings
UNIFI_PORT=8443                # Default: None (uses standard HTTP/HTTPS ports)
UNIFI_VERIFY_SSL=false         # Default: false
UNIFI_TIMEOUT=10              # Default: 10 seconds
UNIFI_SITE=default            # Default: "default"
UNIFI_API_VERSION=v1          # Default: "v1", Options: "v1", "v2"

# Logging Configuration
ISMINET_LOG_LEVEL=DEBUG       # Default: "INFO", Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
ISMINET_DEV_MODE=1           # Default: 0 (false), Set to 1 for development mode
ISMINET_LOG_TO_FILE=1        # Default: 1 (true), Set to 0 to disable file logging

# Test Configuration
# These are used only in test environments
TEST_API_KEY=test_key         # Default API key for tests
TEST_HOST=test.host           # Default host for tests
TEST_PORT=8443                # Default port for tests
